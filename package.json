{
  "name": "es2017-sequelize-demo",
  "version": "1.0.0",
  "description": "The project used for Docker and Microservices workshop",
  "main": "services/users/index.js",
  "scripts": {
    "start": "nodemon | bunyan",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db": "NODE_ENV=development scripts/setup-db.sh",
    "resetdb": "NODE_ENV=development babel-node scripts/initdb.es6",
    "container": "docker build -t $C9_USER/users -f dockerfiles/Dockerfile.users .",
    "startcontainer": "docker run -d -l interlock.hostname=$C9_USER-users -l interlock.domain=apps.containercluster.net -p 8080 --name $C9_USER-users --net taw2016 $C9_USER/users",
    "stopcontainer": "docker rm -f $C9_USER-users"
  },
  "author": "Ilkka Anttonen",
