  "dependencies": {
    "mysql": "^2.11.1",
    "sequelize": "^3.24.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "bunyan": "^1.8.4",
    "nodemon": "^1.11.0",
    "restify": "^4.2.0"  }
}
